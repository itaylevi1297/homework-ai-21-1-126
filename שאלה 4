count = 0
highest = None
lowest = None

while count < 10:
    rating = int(input("Enter rating (1-5): "))

    if rating < 1 or rating > 5:
        continue

    if highest is None or rating > highest:
        highest = rating

    if lowest is None or rating < lowest:
        lowest = rating

    count = count + 1

print("Highest rating:", highest)
print("Lowest rating:", lowest)
